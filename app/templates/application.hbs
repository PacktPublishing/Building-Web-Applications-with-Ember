<h1>{{welcomeText}}</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Description</th>
      <th>Date</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
    {{#each lineItems as |lineItem|}}
      <tr class="{{unless lineItem.isExpense 'info'}}">
        <td>{{lineItem.description}}</td>
        <td>{{lineItem.date}}</td>
        <td>{{lineItem.amount}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}